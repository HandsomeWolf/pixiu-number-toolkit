{
  "name": "pixiu-number-toolkit",
  "version": "3.0.9",
  "description": "A collection of number utilities.",
  "type": "module",
  "keywords": [
    "thousand separator",
    "numeric",
    "decimal",
    "number formatting",
    "math",
    "number conversion",
    "number utilities",
    "typescript"
  ],
  "license": "MIT",
  "homepage": "https://github.com/HandsomeWolf/pixiu-number-toolkit",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HandsomeWolf/pixiu-number-toolkit"
  },
  "author": "辰火流光 <55856773@qq.com> (https://github.com/handsomewolf)",
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./validation": {
      "import": "./dist/validation/index.js",
      "require": "./dist/validation/index.cjs"
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "test": "jest",
    "build": "tsup",
    "build:watch": "tsup --watch",
    "lint": "eslint --fix --ext .ts src/**/*.ts"
  },
  "dependencies": {
    "mathjs": "^13.0.2"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "eslint": "^8.57.0",
    "eslint-config-ali": "^15.0.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.2",
    "tsup": "^8.1.0",
    "typescript": "^5.5.3"
  }
}
